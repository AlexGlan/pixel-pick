{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Pixel Pick",
    "description": "A minimalist eyedropper tool.",
    "author": "alexglan",
    "homepage_url": "https://github.com/AlexGlan/pixel-pick",
    "action": {
        "default_title": "Eyedropper",
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}